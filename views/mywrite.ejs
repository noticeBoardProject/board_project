<!DOCTYPE html>
<html lang="en">

<head>
  <link rel="stylesheet" href="/public/CSS/mywrtie.css" />
  <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>내 글</title>
</head>

<body>
  <!-- 헤더 -->
  <%- include("basicHeader") -%>
    <% console.log(boardData) %>

      <div class="mainwraip">
        <div class="mainbox">
          <h1>내 글</h1>
          <main>
            <div>
              <div class="orderbox">
                <div>전체 글 (<%= boardData.length %>)</div>
                <div class="orderwrap">
                  <img class="gridorder" src="/public/image/gridDisabled.svg" />
                  <img class="reorder" src="/public/image/reorderAbled.svg" />
                </div>
              </div>
              <div class="contentwrap">

                <% boardData.forEach((data)=>{ %>
                  <div class="eachContentwrap">
                    <a href="/main/move/detail/<%= data.id %>">
                      <div class="eachContent">
                        <div class="contentbox">
                          <div class="titlebox">
                            <div class="title">
                              <%= data.title %>
                            </div>
                            <div class="contenttext">
                              <%= data.content %>
                            </div>
                          </div>
                          <div class="boardinfo">
                            <div>
                              <%= data.updatedAt %>
                            </div>
                            <div class="likecount">
                              <div><img src="/public/image/binheart.svg"></div>
                              <div>
                                <%= data.likeCount %>
                              </div>
                            </div>
                          </div>
                        </div>
                        <% if(data.img_url!=="" ){%>
                          <div class="imgbox">
                            <img src="http://localhost:3000/uploads/<%= data.img_url.split(',')[0] %>" />
                          </div>
                          <% } %>
                      </div>
                    </a>
                  </div>
                  <% }) %>


              </div>
            </div>
          </main>
        </div>

      </div>
</body>
<script src="/public/JS/mywrite.js"></script>

</html>